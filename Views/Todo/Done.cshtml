@model TodoViewModel;

@{
    ViewData["Title"] = "Zadania, które wykonałeś!";
}
<h2>Done</h2>

<div class="panel panel-default todo-panel">
    <div class="panel-heading">@ViewData["Title"]</div>
    <table class="table table-hover">
        <thead>
            <tr>
                <td>Zadanie</td>
                <td>Utworzono</td>
            </tr>
        </thead>
        @foreach (var item in Model.ItemsDone)
        {
            <tr>
                <td>@item.Title</td>
                <td>@item.MadeAt</td>
            </tr>
        }
    </table>
</div>